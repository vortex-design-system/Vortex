{
  "semi": true,
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 100,
  "proseWrap": "never",
  "quoteProps": "consistent",
  "singleQuote": true,
  "trailingComma": "all",
  "svelteSortOrder": "scripts-styles-markup",
  "svelteStrictMode": true,
  "svelteBracketNewLine": true,
  "svelteAllowShorthand": false,
  "svelteIndentScriptAndStyle": true
}
